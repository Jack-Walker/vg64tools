jal 0x800FBCB4
move a0, s0

var:
ori a1, r0, 0x14
ori a2, r0, 0x16
jal 0x800FB41C
move a0, s0
lui a1, 0x8014
addiu a1, a1, 0xC410
lui a2, 0x8021
lw a2, 0x3CEC(a2)
lh a2, 0x1C(a2)
jal 0x800FBCB4
move a0, s0

ret:
nop

