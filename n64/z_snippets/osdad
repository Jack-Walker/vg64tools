move a0, s0
addiu a1, r0, 0x0014
jal 0x800FB41C
addiu a2, r0, 0x0010
addiu t7, r0, 0x0040
sw t7, 0x0010 (sp)
move a0, s0
addiu a1, r0, 0x00FF
move a2, r0
jal 0x800FB3AC
addiu a3, r0, 0x00FF
lui a2, 0x8021
lw a2, 0x3CEC(a2)
lui a1, 0x8014
addiu a1, a1, 0xC3D0
jal 0x800FBCB4
move a0, s0

