lw a1, 0x3CEC(a1)
beq a1, r0, ret
nop

Name:
ori a1, r0, 0x14
ori a2, r0, 0x11
jal 0x800FB41C
move a0, s0
lui a1, 0x8014
addiu a1, a1, 0xC3E0
lui a2, 0x8021
lw a2, 0x3CEC(a2)
lw a2, 0x138(a2)
lw a2, 0x18(a2)
jal 0x800FBCB4
move a0, s0

x:
ori a1, r0, 14
ori a2, r0, 12
jal 0x800FB41C
move a0, s0
lui a1, 8014
addiu a1, a1, C3E8
lui a2, 8021
lw a2, 3CEC(a2)
lwc1 $f4, 24(a2)
cvt.w.s $f6, $f4
mfc1 a2, $f6
jal 0x800FBCB4
move a0, s0

y:
ori a1, r0, 0x14
ori a2, r0, 0x13
jal 0x800FB41C
move a0, s0
lui a1, 0x8014
addiu a1, a1, 0xC3F0
lui a2, 0x8021
lw a2, 0x3CEC(a2)
lwc1 $f4, 0x28(a2)
cvt.w.s $f6, $f4
mfc1 a2, $f6
jal 0x800FBCB4
move a0, s0

z:
ori a1, r0, 14
ori a2, r0, 14
jal 0x800FB41C
move a0, s0
lui a1, 0x8014
addiu a1, a1, 0xC3F8
lui a2, 0x8021
lw a2, 0x3CEC(a2)
lwc1 $f4, 0x2C(a2)
cvt.w.s $f6, $f4
mfc1 a2, $f6
jal 0x800FBCB4
move a0, s0

typ:
ori a1, r0, 0x14
ori a2, r0, 0x15
jal 0x800FB41C
move a0, s0
lui a1, 0x8014
addiu a1, a1, 0xC400
lui a2, 0x8021
lw a2, 0x3CEC(a2)
